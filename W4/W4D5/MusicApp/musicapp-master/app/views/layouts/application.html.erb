<!DOCTYPE html>
<html>
  <head>
    <title>MusicApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if current_user %>
      <p>Welcome, <%= current_user.email %>! </p>
      <%= link_to 'logout', session_url, :method => :delete %>

    <% end %>
    <%= yield %>
  </body>
</html>
